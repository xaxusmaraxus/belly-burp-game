; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "SmoothPath",
"language": "GDScript",
"path": "res://smooth_path_modified.gd"
}, {
"base": "Node",
"class": "SoundQueueManager",
"language": "GDScript",
"path": "res://Burps/SoundQueueManager.gd"
} ]
_global_script_class_icons={
"SmoothPath": "",
"SoundQueueManager": ""
}

[application]

config/name="Advanced Match 3 Sample"
run/main_scene="res://Game/Game.tscn"
config/icon="res://Assets/Aliens/shipYellow_manned.png"

[display]

window/size/width=720
window/size/height=1080
window/size/test_width=720
window/size/test_height=1080
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/DYNAMIC_WATER/plugin.cfg", "res://addons/water2d/plugin.cfg" )

[input]

touch={
"deadzone": 0.5,
"events": [ Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"button_mask":0,"position":Vector2( 0, 0 ),"global_position":Vector2( 0, 0 ),"factor":1.0,"button_index":1,"pressed":false,"doubleclick":false,"script":null)
 ]
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
environment/default_environment="res://default_env.tres"
